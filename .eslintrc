{
    "parser": "babel-eslint",

    "plugins": [
        "react",
    ],

    "env": {
        "browser": true,
        "node": true,
    },

    "ecmaFeatures": {
        "jsx": true,
    },

    "rules": {
        "react/jsx-uses-react": 2,
        "strict": 0,
        "quotes": [2, "single"],
        "eol-last": [0],
        "no-underscore-dangle": [0],
        "no-undef": [2],
        "no-unused-vars": [2],
    }
}
